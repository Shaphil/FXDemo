<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" prefHeight="393.0" prefWidth="337.0" spacing="20.0" xmlns="http://javafx.com/javafx/21"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.shaphil.fxdemo.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <CheckBox fx:id="checkBox" mnemonicParsing="false" text="CheckBox"/>
    <Label fx:id="welcomeText" alignment="CENTER" prefHeight="22.0" prefWidth="220" text=" "/>
    <Button onAction="#onHelloButtonClick" text="Done"/>


</VBox>
